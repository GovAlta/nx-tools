{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "id": "NxOcWorkspace",
  "title": "",
  "type": "object",
  "properties": {
    "pipeline": {
      "type": "string",
      "description": "",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What should be the name of the oc pipeline?"
    },
    "infra": {
      "type": "string",
      "description": "",
      "$default": {
        "$source": "argv",
        "index": 1
      },
      "alias": "i",
      "x-prompt": "What is the oc project used for build infrastructure?"
    },
    "dev": {
      "type": "string",
      "description": "",
      "$default": {
        "$source": "argv",
        "index": 2
      },
      "alias": "d",
      "x-prompt": "What is the oc project use as a dev environment?"
    }
  },
  "required": ["pipeline", "infra", "dev"]
}
