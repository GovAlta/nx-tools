{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "id": "NxOcDeployment",
  "title": "",
  "type": "object",
  "properties": {
    "project": {
      "type": "string",
      "description": "Project to add deployment in.",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "Which project to add deployment for?"
    },
    "tenant": {
      "type": "string",
      "description": "Name of the ADSP tenant.",
      "$default": {
        "$source": "argv",
        "index": 1
      },
      "alias": "t",
      "x-prompt": "What is the name of your ADSP tenant?"
    }
  },
  "required": ["project", "tenant"]
}
