{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "id": "NxAdspDotnetService",
  "title": "",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Name of the service.",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What name would you like to use?"
    },
    "tenant": {
      "type": "string",
      "description": "Name of the ADSP tenant.",
      "$default": {
        "$source": "argv",
        "index": 1
      },
      "alias": "t",
      "x-prompt": "What is the name of your ADSP tenant?"
    },
    "namespace": {
      "type": "string",
      "description": "Default namespace to use.",
      "alias": "n"
    }
  },
  "required": ["name", "tenant"]
}
