{
  "npmScope": "abgov",
  "affected": {
    "defaultBase": "origin/main"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    ".github/**/*.yml": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "nx-adsp": {
      "tags": []
    },
    "nx-adsp-e2e": {
      "tags": [],
      "implicitDependencies": ["nx-adsp"]
    },
    "nx-dotnet": {
      "tags": []
    },
    "nx-dotnet-e2e": {
      "tags": [],
      "implicitDependencies": ["nx-dotnet"]
    },
    "nx-oc": {
      "tags": []
    },
    "nx-oc-e2e": {
      "tags": [],
      "implicitDependencies": ["nx-oc"]
    },
    "nx-release": {
      "tags": []
    },
    "nx-release-e2e": {
      "tags": [],
      "implicitDependencies": ["nx-release"]
    }
  },
  "workspaceLayout": {
    "appsDir": "e2e",
    "libsDir": "packages"
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  }
}
