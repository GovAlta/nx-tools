{
  "version": 2,
  "projects": {
    "nx-adsp": {
      "root": "packages/nx-adsp",
      "sourceRoot": "packages/nx-adsp/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/nx-adsp/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/packages/nx-adsp"
          ],
          "options": {
            "jestConfig": "packages/nx-adsp/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/packages/nx-adsp",
            "tsConfig": "packages/nx-adsp/tsconfig.lib.json",
            "packageJson": "packages/nx-adsp/package.json",
            "main": "packages/nx-adsp/src/index.ts",
            "assets": [
              "packages/nx-adsp/*.md",
              {
                "input": "./packages/nx-adsp/src",
                "glob": "**/*.!(ts)",
                "output": "./src"
              },
              {
                "input": "./packages/nx-adsp",
                "glob": "generators.json",
                "output": "."
              },
              {
                "input": "./packages/nx-adsp",
                "glob": "executors.json",
                "output": "."
              }
            ]
          }
        },
        "release": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "npx semantic-release -e ../../../.releaserc.json --tag-format='nx-adsp-v${version}'",
            "cwd": "dist/packages/nx-adsp"
          }
        }
      }
    },
    "nx-adsp-e2e": {
      "projectType": "application",
      "root": "e2e/nx-adsp-e2e",
      "sourceRoot": "e2e/nx-adsp-e2e/src",
      "targets": {
        "e2e": {
          "executor": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "nx-adsp:build",
            "npmPackageName": "@abgov/nx-adsp",
            "pluginOutputPath": "dist/packages/nx-adsp",
            "jestConfig": "e2e/nx-adsp-e2e/jest.config.js"
          }
        }
      }
    },
    "nx-oc": {
      "root": "packages/nx-oc",
      "sourceRoot": "packages/nx-oc/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/nx-oc/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/packages/nx-oc"
          ],
          "options": {
            "jestConfig": "packages/nx-oc/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/packages/nx-oc",
            "tsConfig": "packages/nx-oc/tsconfig.lib.json",
            "packageJson": "packages/nx-oc/package.json",
            "main": "packages/nx-oc/src/index.ts",
            "assets": [
              "packages/nx-oc/*.md",
              {
                "input": "./packages/nx-oc/src",
                "glob": "**/*.!(ts)",
                "output": "./src"
              },
              {
                "input": "./packages/nx-oc",
                "glob": "generators.json",
                "output": "."
              },
              {
                "input": "./packages/nx-oc",
                "glob": "executors.json",
                "output": "."
              }
            ]
          }
        },
        "release": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "npx semantic-release -e ../../../.releaserc.json --tag-format='nx-oc-v${version}'",
            "cwd": "dist/packages/nx-oc"
          }
        }
      }
    },
    "nx-oc-e2e": {
      "projectType": "application",
      "root": "e2e/nx-oc-e2e",
      "sourceRoot": "e2e/nx-oc-e2e/src",
      "targets": {
        "e2e": {
          "executor": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "nx-oc:build",
            "npmPackageName": "@abgov/nx-oc",
            "pluginOutputPath": "dist/packages/nx-oc",
            "jestConfig": "e2e/nx-oc-e2e/jest.config.js"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  }
}